{
  "name": "winston-transport-logtail",
  "version": "1.0.0",
  "description": "logtail.com transport for winston v3 logger",
  "main": "dist/cjs/index.js",
  "module": "dist/es6/index.js",
  "types": "dist/es6/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build:cjs": "tsc",
    "build:es6": "tsc -p tsconfig.es6.json",
    "build": "yarn build:cjs && yarn build:es6",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtso/winston-transport-logtail.git"
  },
  "keywords": [
    "logtail",
    "logger",
    "logging",
    "node",
    "winston",
    "winston-transport",
    "transport",
    "winston3"
  ],
  "author": "mtso <~@mtso.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mtso/winston-transport-logtail/issues"
  },
  "homepage": "https://github.com/mtso/winston-transport-logtail#readme",
  "engine": {
    "node": "16"
  },
  "dependencies": {
    "stack-trace": "^1.0.0-pre1",
    "winston-transport": "^4.4.2"
  },
  "devDependencies": {
    "@logtail/node": "^0.1.10",
    "@logtail/types": "^0.1.8",
    "@types/node": "^17.0.13",
    "typescript": "^4.5.5",
    "winston": "^3.5.0"
  },
  "peerDependencies": {
    "@logtail/node": "^0.1.10",
    "winston": "^3.5.0"
